{
  "notifications": {
    "success_notification_command": [
      "bash",
      "-lc",
      "date >> /tmp/clipai_notify.log; echo success >> /tmp/clipai_notify.log; touch /tmp/clipai_success"
    ],
    "failure_notification_command": [
      "bash",
      "-lc",
      "date >> /tmp/clipai_notify.log; echo failure >> /tmp/clipai_notify.log; touch /tmp/clipai_failure"
    ],
    "duration_in_seconds": 3,
    "hide_notification_command": [
      "bash",
      "-lc",
      "date >> /tmp/clipai_notify.log; echo hide >> /tmp/clipai_notify.log; touch /tmp/clipai_hide"
    ]
  }
}
